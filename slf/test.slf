// default package

terminals S

syntax
    n ::= 0
       | S n

judgment equal: n = n

    ------ eq
     n = n
     
theorem equal-reflexive:
    forall n
    exists n = n
    proof by rule eq
end theorem

theorem equal-symmetric:
    forall e: n1 = n2
    exists n2 = n1
    proof by case analysis on e:
        case rule
            ------- eq
            _: n1 = n1
            where n2 := n1
        is
            proof by rule eq
        end case
    end case analysis
end theorem

theorem equal-transitive:
    forall e: n1 = n2
    forall f: n2 = n3
    exists n1 = n3
    proof by unproved
end theorem
